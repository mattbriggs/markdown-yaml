{
  "compAlertNote": ":::note\\n([\\s\\S]+?)\\n:::",
  "compAlertWarning": ":::warning\\n([\\s\\S]+?)\\n:::",
  "compBlockCode": "```[a-zA-Z0-9]*\\n([\\s\\S]+?)\\n```",
  "compBlockQuote": "(?:^>.*(?:\\n|$))+",
 "compListOrdered": "(^\\s*\\d+\\.\\s+.+(?:\\n\\s+.+)*)",
"compListUnordered": "(^\\s*[-*+]\\s+.+(?:\\n\\s+.+)*)",
"compParagraph": "(?:^(?!#|>|-|\\d+\\.|:::|```|\\||!\\[).+(?:\\n(?!#|>|-|\\d+\\.|:::|```|\\||!\\[).+)*)",
  "compTable": "(^\\|.*\\|\\s*\\n\\|[-:\\s|]+\\|\\n(?:\\|.*\\|\\n?)+)",
  "compImage": "!\\[[\\s\\S]*?\\]\\((.*?)\\)",
  "compVideo": ":::video\\n([\\s\\S]+?)\\n:::",
  "compInclude": "\\[!INCLUDE.*?\\]\\((.*?)\\)"
}